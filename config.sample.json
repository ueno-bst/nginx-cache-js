{
    "name": "localhost",
    "description": "localhost description",
    "server": {
        "host": "localhost2",
        "alias": [
            "localhost.localdomain"
        ]
    },
    "location": [
        {
            "name": "wp-meta",
            "description": "Wordpress meta files",
            "uri": [
                "^/wp-include/.*$",
                "^/wp-content/.*$",
                "^/wp-admin/load-(styles|scripts)\\.php$",
                "^.*\\.(css|js|png|jpe?g|gif|webp|avif|gz|br|ico)$"
            ],
            "cache": {
                "expire": {
                    "min": 0,
                    "max": 31536000
                },
                "rule": {
                    "header": {
                        "type": "none",
                        "pattern": [
                            "accept-encoding"
                        ]
                    },
                    "args": {
                        "type": "all",
                        "pattern": [
                        ]
                    },
                    "cookie": {
                        "type": "none"
                    }
                }
            }
        },
        {
            "name": "njs-test",
            "description": "ngsadsad",
            "uri": [
                "^/njs/.*"
            ],
            "cache": {
                "expire": {
                    "min": 0,
                    "max": 31536000
                },
                "rule": {
                    "args": {
                        "type": "include",
                        "pattern": [
                            "aaa"
                        ]
                    },
                    "header": {
                        "type": "include",
                        "pattern": [
                            "Accept-Encoding",
                            "User_Agent"
                        ]
                    },
                    "cookie": {
                        "type": "include",
                        "pattern": [
                            "lately",
                            "wp_lang",
                            "wordpress_*"
                        ]
                    }
                }
            }
        }
    ]
}
