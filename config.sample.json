{
    "name": "localhost",
    "description": "localhost description",
    "debug": true,
    "server": {
        "host": "localhost",
        "alias": [
            "localhost.localdomain"
        ]
    },
    "location": [
        {
            "name": "meta",
            "description": "Wordpress meta files",
            "uri": [
                "^/wp-include/.*$",
                "^/wp-content/.*$",
                "^/wp-admin/load-(styles|scripts)\\.php$",
                "^.*\\.(css|js|png|jpe?g|gif|webp|avif|gz|br|ico|svg)$"
            ],
            "cache": {
                "expire": {
                    "min": 0,
                    "max": 31536000
                },
                "rule": {
                    "header": {
                        "type": "none",
                        "pattern": [
                            "accept-encoding"
                        ]
                    },
                    "args": {
                        "type": "all",
                        "pattern": [
                        ]
                    },
                    "cookie": {
                        "type": "none"
                    }
                }
            }
        },
        {
            "name": "admin",
            "description": "ngsadsad",
            "uri": [
                "^/wp-admin/"
            ]
        },
        {
            "name": "front",
            "description": "ngsadsad",
            "uri": [
                ".*"
            ],
            "cache": {
                "disable": false,
                "rule": {
                    "args": {
                        "type": "all"
                    },
                    "header": {
                        "type": "include",
                        "pattern": [
                            "authorization"
                        ]
                    },
                    "cookie": {
                        "type": "include",
                        "pattern": [
                            "wordpress_logged_in_*",
                            "comment_*",
                            "wp-settings-*"
                        ]
                    }
                }
            }
        }
    ]
}
